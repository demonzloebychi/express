{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }

  ],
  "routes": [
    { "src": "/(.*\\.html)", "dest": "/api/index.js" },
    { "src": "/api/call", "methods": ["POST"], "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
